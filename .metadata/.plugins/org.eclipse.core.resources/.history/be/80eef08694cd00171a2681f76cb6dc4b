package stringCalculator;

import static org.junit.Assert.*;

import org.junit.Test;

public class StringCalculatorOneTest {
	
	StringCalculatorOne stringCalculatorOne = new StringCalculatorOne();
	private String ACTUAL_STRING_ONE = "0";
	private int EXPECTED_STRING_ONE = 0;

	@Test
	public void testRequirementOne() {
		assertEquals(EXPECTED_STRING_ONE, stringCalculatorOne.sumItUp(ACTUAL_STRING_ONE));
	}

}
