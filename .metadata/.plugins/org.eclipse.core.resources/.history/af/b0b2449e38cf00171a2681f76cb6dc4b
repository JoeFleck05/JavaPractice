package stringCalculatorThree;

public class StringCalculator {

	public int add(String value) {
		int rtnNum;
		int tempNum = 0;
		String delimiterString = "//";
		
		if(value.contains(delimiterString)) {
			
		}
		String[] strArr = value.split(",|\n");
		
		for(String s : strArr) {
			tempNum += Integer.parseInt(s); 
		}
		rtnNum = tempNum;
		return rtnNum;
	}
}
